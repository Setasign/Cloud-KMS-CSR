{
    "name": "setasign/cloud-kms-csr",
    "homepage": "https://github.com/Setasign/Cloud-KMS-CSR",
    "description": "Tools to create and update certificate signing requests (CSRs) and self-signed certificates with keys from cloud key management systems (AWS KMS and Google Cloud KMS)",
    "type": "library",
    "keywords": [
        "csr",
        "x509",
        "kms",
        "cloud",
        "aws",
        "google-cloud"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "setasign\\CloudKmsCsr\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "setasign\\CloudKmsCsr\\tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "ext-openssl": "*",
        "aws/aws-sdk-php": "^3.171",
        "setasign/setapdf-signer": "^2.0",
        "google/cloud-kms": "^1.11",
        "phpseclib/phpseclib": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://www.setasign.com/downloads/"
        }
    ],
    "authors": [
        {
            "name": "Jan Slabon",
            "email": "jan.slabon@setasign.com",
            "homepage": "https://www.setasign.com"
        }
    ]
}
